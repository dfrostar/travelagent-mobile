{
  "name": "travelagent_mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:build": "detox build",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc",
    "prepare": "husky install",
    "env:switch": "node scripts/switch-env.js",
    "env:dev": "node scripts/switch-env.js dev",
    "env:staging": "node scripts/switch-env.js staging",
    "env:prod": "node scripts/switch-env.js prod"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.0",
    "@sentry/react-native": "^5.0.0",
    "@shopify/react-native-skia": "^0.1.241",
    "@tanstack/react-query": "^4.0.0",
    "axios": "^1.6.0",
    "expo": "~50.0.4",
    "expo-av": "~13.10.4",
    "expo-file-system": "~16.0.5",
    "expo-permissions": "~14.2.1",
    "expo-status-bar": "~1.11.1",
    "fft.js": "^4.0.4",
    "i18next": "^23.16.8",
    "lottie-react-native": "^6.7.2",
    "openai": "^4.79.1",
    "react": "18.2.0",
    "react-i18next": "^14.1.3",
    "react-native": "0.73.2",
    "react-native-dotenv": "^3.4.9",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "react-native-reanimated": "^3.16.7",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.0"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
